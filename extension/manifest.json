{
    "name": "Youtube Music Controller",
    "description": "youtube music controller",
    "icons": {
        "128": "icon.png"
    },
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://music.youtube.com/*"],
            "js": ["ym_control.js"]
        },
        {
            "matches": ["https://www.twitch.tv/*"],
            "js": ["tw_control.js"]
        }
    ]
}