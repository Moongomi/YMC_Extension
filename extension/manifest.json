{
    "name": "Youtube Music Controller",
    "description": "youtube music controller",
    "icons": {
        "128": "icon.png"
    },
    "version": "0.4",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://music.youtube.com/*"],
            "js": ["control.js"]
        }
    ]
}